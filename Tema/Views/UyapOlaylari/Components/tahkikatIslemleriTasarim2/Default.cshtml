  <div class="panel panel-info">
  <div class="panel-heading">Tahkikat İşlemleri</div>
  <div class="panel-body">
                <button type="button" id="B_olayYeriTesbitTutatagi" class="btn btn-primary">Olay Yeri Tespit Tutanağı <span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span>  </button>
                <button type="button" id="B_ifadeAlmaIslemleri" class="btn btn-primary">İfade Alma İşlemleri <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> </button>
                <button type="button" id="B_aileIciSiddetOlaylari" class="btn btn-primary">Aile İçi Şiddet Olayları  <span class="glyphicon glyphicon-plus-sign" aria-hidden="true"></span></button>
                <button type="button" id="B_savciGorusmeTutanagi" class="btn btn-primary">Savcı Görüşme Tutanağı <span class="glyphicon glyphicon-edit" aria-hidden="true"></span> </button>
                <button type="button" id="B_degerTesbitTutatagi" class="btn btn-primary">İlgili Kurum Değer Tespit Tutanağı <span class="glyphicon glyphicon-file" aria-hidden="true"></span></button>

  </div>
</div>


<div  class="col-lg-12" style="margin:5px;">
                    <div id="olayYeriTesbitTutatagi_view" style="display: none;">
                        @await Component.InvokeAsync("olayYeriTesbitTutanagi")
                    </div>
                    <div id="ifadeAlmaIslemleri_view"  style="display: none;">
                        @await Component.InvokeAsync("ifadeAlmaIslemleri")
                    </div>
                    <div id="aileIciSiddetOlaylari_view"  style="display: none;">
                        @await Component.InvokeAsync("aileIciSiddetOlaylari")
                    </div>
                     <div id="savciGorusmeTutanagi_view"  style="display: none;">
                        @await Component.InvokeAsync("savciGorusmeTutanagi")
                    </div>
                    <div id="degerTesbitTutatagi_view"  style="display: none;">
                        @await Component.InvokeAsync("degerTesbitTutatagi")
                    </div>
</div>   

  